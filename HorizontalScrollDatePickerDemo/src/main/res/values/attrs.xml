<?xml version="1.0" encoding="utf-8"?>
<resources>


    <declare-styleable name="HorizontalDateFilter">
        <attr name="textColor" format="color" />
        <attr name="itemColor" format="color" />
        <attr name="itemColorPressed" format="color" />
        <attr name="background" format="color" />
        <attr name="dateToPick" format="enum">
            <enum name="year" value="3" />
            <enum name="month" value="2" />
            <enum name="week" value="1" />
            <enum name="day" value="0" />
        </attr>
    </declare-styleable>




</resources>